<div class="tile">
  <h3 class="tile-title">Agency Stability</h3>
  <div class="tile-body">
    <div class="form-horizontal">
      <div class="form-group row">
        <label class="control-label col-md-3">Year Founded</label>
        <div class="col-md-8">
          <%= form.number_field :founded, class: "form-control" %>
        </div>
      </div>
      <div class="form-group row">
        <label class="control-label col-md-3">Form 990 Filed</label>
        <div class="col-md-8">
          <%= form.check_box :form_990, class: "form-control form-check-input" %>
        </div>
      </div>
      <div class="form-group row">
        <label class="control-label col-md-3">Form 990</label>
        <% if partner.proof_of_form_990.attached? %>
          <div class="col-md-8">
            Attached
            file: <%= link_to partner.proof_of_form_990.blob['filename'], rails_blob_path(partner.proof_of_form_990), class: "font-weight-bold" %>
            <%= form.file_field :proof_of_form_990, class: "form-control-file form-control" %>
          </div>
        <% else %>
          <div class="col-md-8">
            <%= form.file_field :proof_of_form_990, class: "form-control-file" %>
          </div>
        <% end %>
        </div>
        <div class="form-group row">
          <label class="control-label col-md-3">Program Name</label>
          <div class="col-md-8">
            <%= form.text_field :program_name, class: "form-control" %>
          </div>
        </div>
        <div class="form-group row">
          <label class="control-label col-md-3">Program Description</label>
          <div class="col-md-8">
            <%= form.text_area :program_description, class: "form-control" %>
          </div>
        </div>
        <div class="form-group row">
          <label class="control-label col-md-3">Agency Age</label>
          <div class="col-md-8">
            <%= form.text_field :program_age, class: "form-control" %>
          </div>
        </div>
        <div class="form-group row">
          <label class="control-label col-md-3">Do You Track Program Results</label>
          <div class="col-md-8">
            <%= form.check_box :evidence_based, class: "form-control form-check-input" %>
          </div>
        </div>
        <div class="form-group row">
          <label class="control-label col-md-3">Case Management</label>
          <div class="col-md-8">
            <%= form.check_box :case_management, class: "form-control form-check-input" %>
          </div>
        </div>
        <div class="form-group row">
          <label class="control-label col-md-3">Evidence Based Description</label>
          <div class="col-md-8">
            <%= form.text_area :evidence_based_description, class: "form-control" %>
          </div>
        </div>
        <div class="form-group row">
          <label class="control-label col-md-3">Verified Successes of Program</label>
          <div class="col-md-8">
            <%= form.text_area :program_client_improvement, class: "form-control" %>
          </div>
        </div>
        <div class="form-group row">
          <label class="control-label col-md-3">Diaper Use</label>
          <div class="col-md-8">
            <%= form.text_field :diaper_use, class: "form-control" %>
          </div>
        </div>
        <div class="form-group row">
          <label class="control-label col-md-3">Do You Receive Diapers From Other Sources</label>
          <div class="col-md-8">
            <%= form.text_field :other_diaper_use, class: "form-control" %>
          </div>
        </div>
        <div class="form-group row">
          <label class="control-label col-md-3">Currently Provide Diapers</label>
          <div class="col-md-8">
            <%= form.check_box :currently_provide_diapers, class: "form-check-input form-control" %>
          </div>
        </div>
        <div class="form-group row">
          <label class="control-label col-md-3">Turn Away Child Care</label>
          <div class="col-md-8">
            <%= form.check_box :turn_away_child_care, class: "form-check-input form-control" %>
          </div>
        </div>
    </div>
  </div>
</div>
<div class="tile">
  <h3 class="tile-title">Program Address</h3>
  <div class="tile-body">
    <div class="form-horizontal">
      <div class="form-group row">
        <label class="control-label col-md-3">Address 1</label>
        <div class="col-md-8">
          <%= form.text_field :program_address1, class: "form-control" %>
        </div>
      </div>
      <div class="form-group row">
        <label class="control-label col-md-3">Address 2</label>
        <div class="col-md-8">
          <%= form.text_field :program_address2, class: "form-control" %>
        </div>
      </div>
      <div class="form-group row">
        <label class="control-label col-md-3">City</label>
        <div class="col-md-8">
          <%= form.text_field :program_city, class: "form-control" %>
        </div>
      </div>
      <div class="form-group row">
        <label class="control-label col-md-3">State</label>
        <div class="col-md-8">
          <%= form.text_field :program_state, class: "form-control" %>
        </div>
      </div>
      <div class="form-group row">
        <label class="control-label col-md-3">Zip Code</label>
        <div class="col-md-8">
          <%= form.number_field :program_zip_code, class: "form-control" %>
        </div>
      </div>
    </div>
  </div>
</div>
